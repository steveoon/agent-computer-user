{
  "name": "ai-sdk-computer-use",
  "version": "1.29.0-develop.6",
  "private": true,
  "main": "build/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:sh": "shellcheck scripts/*.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:mcp-connection": "npx tsx examples/puppeteer-usage.ts",
    "test:semantic-release": "node scripts/test-semantic-release-sync.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:sh": "shfmt -w -i 4 scripts/*.sh",
    "semantic-release": "semantic-release",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "agent": "scripts/multi-agent.sh",
    "agent:add": "scripts/multi-agent.sh add",
    "agent:list": "scripts/multi-agent.sh list",
    "agent:start": "scripts/multi-agent.sh start",
    "agent:stop": "scripts/multi-agent.sh stop",
    "agent:restart": "scripts/multi-agent.sh restart",
    "agent:status": "scripts/multi-agent.sh status",
    "agent:logs": "scripts/multi-agent.sh logs",
    "agent:update": "scripts/multi-agent.sh update",
    "agent:remove": "scripts/multi-agent.sh remove",
    "agent:clean-db": "scripts/multi-agent.sh clean-indexeddb",
    "monitor:start": "npx tsx scripts/unread-monitor.ts",
    "monitor:start:auto": "AUTO_SUBMIT=true npx tsx scripts/unread-monitor.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:init": "npx tsx scripts/init-brand-data.ts",
    "electron:dev": "concurrently \"next dev\" \"tsc -w -p tsconfig-electron.json\" \"wait-on http://localhost:3000 && cross-env ELECTRON_RUN_AS_NODE= NODE_ENV=development electron .\"",
    "electron:build": "cross-env ELECTRON_BUILD=true next build && shx cp -r .next/static .next/standalone/.next/ && shx mkdir -p .next/standalone/public && shx cp -r public/* .next/standalone/public/ && node scripts/resolve-standalone.js && tsc -p tsconfig-electron.json",
    "electron:start": "cross-env ELECTRON_RUN_AS_NODE= electron .",
    "electron:pack": "pnpm electron:build && electron-builder --dir",
    "electron:dist": "pnpm electron:build && electron-builder",
    "electron:dist:mac": "pnpm electron:build && electron-builder --mac",
    "electron:dist:win": "pnpm electron:build && electron-builder --win"
  },
  "dependencies": {
