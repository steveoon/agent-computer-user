    "fast-deep-equal": "^3.1.3",
    "js-tiktoken": "^1.0.20",
    "localforage": "^1.10.0",
    "lucide-react": "^0.479.0",
    "motion": "^12.4.10",
    "next": "15.4.10",
    "next-electron-rsc": "^0.3.0",
    "next-themes": "^0.4.5",
    "postgres": "^3.4.7",
    "qwen-ai-provider": "^0.1.0",
    "react": "19.1.4",
    "react-day-picker": "^9.13.0",
